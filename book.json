{
    "title": "学C语言写Lisp",
    "language": "zh-Hans",
    "plugins": ["disqus", "page-toc", "theme-gestalt", "-theme-default", "styles-sass-fix"],
    "pluginsConfig": {
        "page-toc": {
            "selector": ".markdown-section h2",
            "position": "top",
            "showByDefault": true
        },
        "disqus": {
            "shortName": "buildyourownlispcn"
        },
        "theme-gestalt": {
            "logo": "/assets/logo.png",
            "favicon": "/assets/logo.png",
            "baseUrl": null,
            "excludeDefaultStyles": true,
            "doNotHideChildrenChapters" : false
        }
    },
    "styles": {
        "website": "./assets/styles/website.scss"
    }
}
